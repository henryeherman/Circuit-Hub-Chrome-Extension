{
  "name" : "CircuitHub",
  "version" : "1.0",
  "description" : "Link To Parts in CircuitHub",
  "background_page" : "background.html",
  "page_action" :
  {
    "default_icon" : "logo-19.png",
    "default_title" : "Locate Parts in Page",
	"popup": "popup.html"
  },
  "content_scripts" : [
    {
      "matches" : [
        "http://search.digikey.com/us/en/*",
        "https://search.digikey.com/us/en/*"
      ],
      "js" : ["contentscript.js"],
      "run_at" : "document_end",
      "all_frames" : false
    }
  ],
  "icons" : {
    "48" : "logo-48.png",
    "128" : "logo-128.png"
  },
  "permissions": [
  "http://api.flickr.com/",
  "http://circuithub.com/"
  ]
}
